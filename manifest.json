{
  "manifest_version":2,

  "name": "Hotkey",
  "description": "Shortcut to checking out",
"options_page": "options.html",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands":{
     "_execute_browser_action":{
	"suggested_key":{
	"default":"Ctrl+Shift+Y",
        "mac":"Command+Shift+Y"
        },
	"description":"browser"
     }
  },
  "browser_action":{
	"default_icon":"icon.png",
	"default_popup":"popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}